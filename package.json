{
  "name": "getashell",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next experimental-compile",
    "build:image": "docker buildx build -t getashell:testing .",
    "clean:containers": "docker rm -f $(docker ps -aq)",
    "clean:keys": "ssh-keygen -f ~/.ssh/known_hosts -R [localhost]:2222",
    "start": "next start",
    "lint": "next lint",
    "tsx": "tsx",
    "drizzle-kit": "drizzle-kit",
    "migrate:generate": "drizzle-kit  generate:sqlite --config drizzle.config.json",
    "migrate": "mkdir -p data/ && drizzle-kit push:sqlite --config drizzle.config.json"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.0.0",
    "@typescript-eslint/typescript-estree": "^7.5.0",
    "better-sqlite3": "^9.4.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.4",
    "iron-session": "^8.0.1",
    "next": "14.1.3",
    "next-safe-action": "^6.2.0",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-dom": "^18",
    "react-toastify": "^10.0.5",
    "tsx": "^4.7.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.30",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
